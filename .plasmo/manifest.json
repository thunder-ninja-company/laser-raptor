{
  "manifest_version": 3,
  "icons": {
    "16": "./gen-assets/icon16.png",
    "48": "./gen-assets/icon48.png",
    "128": "./gen-assets/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "./gen-assets/icon16.png",
      "48": "./gen-assets/icon48.png"
    },
    "default_popup": "./static/popup/index.html"
  },
  "version": "0.2.0",
  "name": "Browser",
  "description": "A basic Plasmo extension.",
  "author": "danieldonahoe",
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "https://*/*"
  ]
}
