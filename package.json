{
    "name": "thunder-volcano",
    "version": "0.2.0",
    "private": true,
    "displayName": "Browser",
    "description": "A basic Plasmo extension.",
    "author": "danieldonahoe",
    "packageManager": "pnpm@7.1.9",
    "bin": {
        "vibe": "./bin/run"
    },
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "keywords": [
        "oclif"
    ],
    "types": "dist/index.d.ts",
    "main": "./src/electron/main.js",
    "scripts": {
        "dev:next": "npm run open-browser:next && next dev -p 13337 -H next.local",
        "dev:plasmo": "plasmo dev",
        "dev:storybook": "npm run open-browser:storybook && start-storybook -p 6006 -h storybook.local",
        "build:storybook": "build-storybook",
        "build:next": "next build && next export",
        "build:plasmo": "plasmo build",
        "start:electron": "electron .",
        "start:next": "next start",
        "open-browser:storybook": "open http://storybook.local:6006/",
        "open-browser:next": "open http://next.local:13337/",
        "open-browser:electron": "open http://electron.local:13338/",
        "lint:next": "next lint",
        "build:cli": "shx rm -rf dist && tsc -b",
        "postpack": "shx rm -f oclif.manifest.json",
        "posttest": "yarn lint",
        "prepack": "yarn build:cli && oclif manifest && oclif readme",
        "wtf:test": "mocha --forbid-only \"./**/*test.tsx\"",
        "test:cypress": "cypress open",
        "test:jest-0": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "test:jest-1": "NODE_OPTIONS=--experimental-vm-modules npx jest",
        "test:jest-2": "node_modules/jest/bin/jest.js",
        "version": "oclif readme && git add README.md",
        "format": "prettier ./src",
        "check": "prettier ./src",
        "prettier-watch": "onchange \"**/*\" -- prettier --ignore-unknown {{changed}}"
    },
    "overrides": {
        "react-beautiful-dnd": {
            "react": "$react",
            "react-dom": "$react-dom"
        },
        "react-element-to-jsx-string": {
            "react": "$react",
            "react-dom": "$react-dom"
        },
        "@mdx-js/react": {
            "react": "$react",
            "react-dom": "$react-dom"
        },
        "react-inspector": {
            "react": "$react",
            "react-dom": "$react-dom"
        }
    },
    "dependencies": {
        "@mantine/core": "5.1.1",
        "@mantine/form": "5.1.1",
        "@mantine/hooks": "5.1.1",
        "@mantine/next": "5.1.1",
        "@oclif/core": "1.13.8",
        "@oclif/plugin-help": "5.1.12",
        "@oclif/plugin-plugins": "2.1.0",
        "@reduxjs/toolkit": "1.8.3",
        "@tabler/icons": "1.79.0",
        "@types/styled-components": "5.1.25",
        "aws-amplify": "4.3.30",
        "core-js": "3.24.1",
        "lodash": "4.17.21",
        "nanoid": "4.0.0",
        "newrelic": "9.0.0",
        "next": "12.2.4",
        "react": "18.2.0",
        "react-beautiful-dnd": "13.1.0",
        "react-dnd": "16.0.1",
        "react-dnd-html5-backend": "16.0.1",
        "react-dom": "18.2.0",
        "react-redux": "8.0.2",
        "styled-components": "5.3.5",
        "tabler-icons-react": "1.54.0",
        "use-debounce": "8.0.3",
        "web-vitals": "2.1.4"
    },
    "devDependencies": {
        "@babel/core": "7.18.10",
        "@babel/preset-typescript": "7.18.6",
        "@cypress/webpack-preprocessor": "5.12.0",
        "@oclif/test": "2.1.0",
        "@storybook/addon-actions": "6.5.10",
        "@storybook/addon-essentials": "6.5.10",
        "@storybook/addon-interactions": "6.5.10",
        "@storybook/addon-links": "6.5.10",
        "@storybook/builder-webpack5": "6.5.10",
        "@storybook/jest": "0.0.10",
        "@storybook/manager-webpack5": "6.5.10",
        "@storybook/react": "6.5.10",
        "@storybook/testing-library": "0.0.13",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.3.0",
        "@trivago/prettier-plugin-sort-imports": "3.3.0",
        "@types/chai": "4.3.3",
        "@types/chrome": "0.0.193",
        "@types/jest": "28.1.6",
        "@types/mocha": "9.1.1",
        "@types/node": "18.6.5",
        "@types/react": "18.0.17",
        "@types/react-dom": "18.0.6",
        "@typescript-eslint/eslint-plugin": "5.33.0",
        "@typescript-eslint/parser": "5.33.0",
        "babel-jest": "28.1.3",
        "babel-loader": "8.2.5",
        "chai": "4.3.6",
        "crypto": "1.0.1",
        "cypress": "10.4.0",
        "electron": "20.0.1",
        "eslint": "8.21.0",
        "eslint-config-next": "12.2.4",
        "eslint-config-oclif": "4.0.0",
        "eslint-config-oclif-typescript": "1.0.2",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-editorconfig": "4.0.2",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-storybook": "0.6.4",
        "eslint-plugin-unused-imports": "2.0.0",
        "globby": "13.1.2",
        "jest": "28.1.3",
        "jest-environment-jsdom": "28.1.3",
        "mocha": "10.0.0",
        "oclif": "3.1.2",
        "plasmo": "0.50.1",
        "prettier": "2.7.1",
        "prettier-eslint": "15.0.1",
        "shx": "0.3.4",
        "ts-jest": "28.0.7",
        "ts-node": "10.9.1",
        "tsconfig-paths-webpack-plugin": "4.0.0",
        "tslib": "2.4.0",
        "typescript": "4.7.4"
    },
    "manifest": {
        "host_permissions": [
            "https://*/*"
        ],
        "permissions": [
            "tabs"
        ]
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "bugs": "https://github.com/dandonahoe/hello-world/issues",
    "oclif": {
        "bin": "vibe",
        "dirname": "vibe",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " ",
        "topics": {
            "hello": {
                "description": "Say hello to the world and others"
            }
        }
    }
}
