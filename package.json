{
    "name": "thunder-volcano",
    "version": "0.2.0",
    "private": true,
    "displayName": "Browser",
    "description": "A basic Plasmo extension.",
    "author": "danieldonahoe",
    "packageManager": "pnpm@7.1.9",
    "bin": {
        "vibe": "./bin/run"
    },
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "keywords": [
        "oclif"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint:next"
        }
    },
    "overrides": {
        "react-beautiful-dnd": {
            "react": "$react",
            "react-dom": "$react-dom"
        },
        "react-element-to-jsx-string": {
            "react": "$react",
            "react-dom": "$react-dom"
        },
        "@mdx-js/react": {
            "react": "$react",
            "react-dom": "$react-dom"
        },
        "react-inspector": {
            "react": "$react",
            "react-dom": "$react-dom"
        }
    },
    "types": "dist/index.d.ts",
    "main": "./src/electron/main.js",
    "scripts": {
        "dev:next": "npm run open-browser:next && next dev -p 13337 -H next.local",
        "dev:plasmo": "plasmo dev",
        "dev:storybook": "npm run open-browser:storybook && start-storybook -p 6006 -h storybook.local",
        "build:storybook": "build-storybook",
        "build:next": "next build && next export",
        "build:plasmo": "plasmo build",
        "start:electron": "electron .",
        "start:next": "next start",
        "open-browser:storybook": "open http://storybook.local:6006/",
        "open-browser:next": "open http://next.local:13337/",
        "open-browser:electron": "open http://electron.local:13338/",
        "test:cypress": "cypress open",
        "lint:next": "next lint",
        "build:cli": "shx rm -rf dist && tsc -b",
        "postpack": "shx rm -f oclif.manifest.json",
        "posttest": "npm run lint:next",
        "prepack": "yarn build:cli && oclif manifest && oclif readme",
        "test": "mocha --require @babel/register",
        "version": "oclif readme && git add README.md",
        "format": "prettier ./src",
        "check": "prettier ./src",
        "prettier-watch": "onchange \"**/*\" -- prettier --ignore-unknown {{changed}}",
        "prepare": "husky install",
        "commit": "git-cz",
        "commit:1": "commit"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "disableScopeLowerCase": false,
            "disableSubjectLowerCase": false,
            "maxHeaderWidth": 100,
            "maxLineWidth": 100,
            "defaultType": "",
            "defaultScope": "",
            "defaultSubject": "",
            "defaultBody": "",
            "defaultIssues": "",
            "types": {
              "feat": {
                "description": "A new feature",
                "title": "Features"
              }
            }
        }

    },
    "dependencies": {
        "@aws-amplify/ui-react": "2.17.1",
        "@mantine/core": "4.2.5",
        "@mantine/form": "4.2.10",
        "@mantine/hooks": "4.2.5",
        "@mantine/next": "4.2.5",
        "@oclif/core": "1",
        "@oclif/plugin-help": "5",
        "@oclif/plugin-plugins": "2.0.1",
        "@reduxjs/toolkit": "1.8.2",
        "@tabler/icons": "1.78.1",
        "@types/styled-components": "5.1.25",
        "aws-amplify": "4.3.22",
        "nanoid": "4.0.0",
        "newrelic": "8.13.2",
        "next": "12.1.6",
        "react": "18.1.0",
        "react-beautiful-dnd": "13.1.0",
        "react-dnd": "16.0.1",
        "react-dnd-html5-backend": "16.0.1",
        "react-dom": "18.1.0",
        "react-redux": "8.0.2",
        "styled-components": "5.3.5",
        "tabler-icons-react": "1.48.0",
        "use-debounce": "8.0.3",
        "web-vitals": "2.1.4"
    },
    "devDependencies": {
        "@babel/cli": "7.18.10",
        "@babel/core": "7.19.0",
        "@babel/preset-env": "7.19.0",
        "@babel/register": "7.18.9",
        "@commitlint/cli": "17.1.2",
        "@commitlint/config-conventional": "17.1.0",
        "@commitlint/prompt": "17.1.2",
        "@oclif/test": "2",
        "@storybook/addon-actions": "6.5.9",
        "@storybook/addon-essentials": "6.5.9",
        "@storybook/addon-interactions": "6.5.9",
        "@storybook/addon-links": "6.5.9",
        "@storybook/builder-webpack5": "6.5.9",
        "@storybook/jest": "0.0.10",
        "@storybook/manager-webpack5": "6.5.9",
        "@storybook/react": "6.5.9",
        "@storybook/testing-library": "0.0.13",
        "@trivago/prettier-plugin-sort-imports": "3.2.0",
        "@types/chai": "4",
        "@types/chrome": "0.0.188",
        "@types/mocha": "9.0.0",
        "@types/node": "17.0.33",
        "@types/react": "18.0.9",
        "@types/react-dom": "18.0.4",
        "@typescript-eslint/eslint-plugin": "5.36.2",
        "@typescript-eslint/parser": "5.33.0",
        "babel-loader": "8.2.5",
        "chai": "4",
        "commitizen": "4.2.5",
        "cypress": "10.0.0",
        "electron": "19.0.2",
        "eslint": "8.21.0",
        "eslint-config-next": "12.2.4",
        "eslint-config-oclif": "4.0.0",
        "eslint-config-oclif-typescript": "1.0.2",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-editorconfig": "4.0.2",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-storybook": "0.6.4",
        "globby": "11",
        "husky": "8.0.0",
        "mocha": "9",
        "oclif": "3",
        "plasmo": "latest",
        "prettier": "2.6.2",
        "prettier-eslint": "15.0.1",
        "shx": "0.3.3",
        "ts-node": "10.2.1",
        "tsconfig-paths-webpack-plugin": "3.5.2",
        "tslib": "2.3.1",
        "typescript": "4.7.2"
    },
    "manifest": {
        "host_permissions": [
            "https://*/*"
        ],
        "permissions": [
            "tabs"
        ]
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "bugs": "https://github.com/dandonahoe/hello-world/issues",
    "oclif": {
        "bin": "vibe",
        "dirname": "vibe",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " ",
        "topics": {
            "hello": {
                "description": "Say hello to the world and others"
            }
        }
    }
}
